<div class="row page-header">
  <div class="span0">
  <h3>Property info</h3>
  </div>
  <div class="span0">
    <h3>
      <%= link_to edit_property_path(@property, ref:request.path), :class => 'btn btn-default' do %>
        <span class="icon icon-pencil"></span>
        Edit
      <% end %>
      
      <%= link_to properties_path, :class => 'btn btn-default' do %>
        <span class="icon icon-list"></span>
        List
      <% end %>
    </h3>
  </div>
</div>

<p id="notice"><%= notice %></p>

<%= render partial: '/properties/show', locals: {property: @property} %>

<div class="children_list tabbable ">
  <ul class="nav nav-tabs">
  </ul><!-- end-of-tabs -->
  <div class="tab-content">
  </div><!-- end-of-contents -->
</div><!--end-children-list -->
